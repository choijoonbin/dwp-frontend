{
  "name": "@dwp-frontend/workspace",
  "author": "minimals.cc",
  "licence": "MIT",
  "version": "3.0.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/*",
    "apps/remotes/*",
    "libs/*"
  ],
  "scripts": {
    "dev": "vite",
    "dev:dev": "vite --mode dev",
    "dev:mail": "vite --config apps/remotes/mail/vite.config.ts",
    "dev:mail:dev": "vite --mode dev --config apps/remotes/mail/vite.config.ts",
    "dev:all": "npx concurrently \"npm run dev\" \"npm run dev:mail\"",
    "start": "vite preview",
    "build": "tsc && vite build --mode prod",
    "build:mail": "tsc && vite build --mode prod --config apps/remotes/mail/vite.config.ts",
    "lint": "eslint \"apps/**/*.{js,jsx,ts,tsx}\" \"libs/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"apps/**/*.{js,jsx,ts,tsx}\" \"libs/**/*.{js,jsx,ts,tsx}\"",
    "lint:print": "npx eslint --print-config eslint.config.mjs > eslint-show-config.json",
    "fm:check": "prettier --check \"apps/**/*.{js,jsx,ts,tsx}\" \"libs/**/*.{js,jsx,ts,tsx}\"",
    "fm:fix": "prettier --write \"apps/**/*.{js,jsx,ts,tsx}\" \"libs/**/*.{js,jsx,ts,tsx}\"",
    "fix:all": "npm run lint:fix && npm run fm:fix",
    "clean": "rm -rf node_modules .next out dist build",
    "re:dev": "yarn clean && yarn install && yarn dev",
    "re:build": "yarn clean && yarn install && yarn build",
    "re:build-npm": "npm run clean && npm install && npm run build",
    "tsc:dev": "yarn dev & yarn tsc:watch",
    "tsc:watch": "tsc --noEmit --watch",
    "tsc:print": "npx tsc --showConfig"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "yarn@1.22.22",
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource-variable/dm-sans": "^5.2.5",
    "@fontsource/barlow": "^5.2.5",
    "@iconify/react": "^5.2.1",
    "@mui/lab": "^7.0.0-beta.10",
    "@mui/material": "^7.0.1",
    "@tanstack/react-query": "^5.66.6",
    "apexcharts": "^4.5.0",
    "axios": "^1.7.9",
    "dayjs": "^1.11.13",
    "es-toolkit": "^1.34.1",
    "minimal-shared": "^1.0.7",
    "react": "^19.1.0",
    "react-apexcharts": "^1.7.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.4.1",
    "simplebar-react": "^3.3.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@nx/devkit": "^22.3.3",
    "@nx/js": "^22.3.3",
    "@nx/vite": "^22.3.3",
    "concurrently": "^9.1.2",
    "@types/node": "^22.19.5",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "@typescript-eslint/parser": "^8.29.0",
    "@vitejs/plugin-react-swc": "^3.8.1",
    "eslint": "^9.23.0",
    "eslint-import-resolver-typescript": "^4.3.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-perfectionist": "^4.11.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "nx": "^22.3.3",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.29.0",
    "vite": "^6.2.5",
    "vite-plugin-checker": "^0.9.1"
  }
}
